/*
Strings :- String is basically an object that represents sequence of char values.
*/

import java.util.*;

public class git{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        System.out.println("Enroll your first name.");
        String firstName = sc.next();

        System.out.println("Enroll your Last name");
        String lastName = sc.next();

        System.out.println("Your name is " + firstName + " " + lastName);
        System.out.println("Length of firstName is " + firstName.length());

        System.out.println(lastName.charAt(2));          //charAt
    }
}


// compareTo

public class git{
    public static void main(String args[]){
        String n1 = "Ball";
        String n2 = "Football";

        if(n1.compareTo(n2) == 0){
            System.out.println("Strings are Equal");
        }else {
            System.out.println("Strings are not Equal");
        }
    }
}
